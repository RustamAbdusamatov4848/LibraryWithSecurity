{
  "mappings": [
    {
      "request": {
        "method": "POST",
        "urlPattern": "/api/v1/file-storage-management/addBucket/.*"
      },
      "response": {
        "status": 200,
        "jsonBody": {
          "result": {
            "httpStatusCode": "CREATED",
            "status": "SUCCESS",
            "description": "Bucket bucket-example successfully created"
          }
        },
        "headers": {
          "Content-Type": "application/json"
        }
      }
    },
    {
      "request": {
        "method": "GET",
        "url": "/api/v1/file-storage-management/file/download",
        "queryParameters": {
          "fileName": {
            "equalTo": "passport.jpg"
          },
          "bucketName": {
            "equalTo": "bucket-example"
          }
        }
      },
      "response": {
        "status": 200,
        "jsonBody": {
          "result": {
            "httpStatusCode": "OK",
            "status": "SUCCESS",
            "description": "File passport.jpg successfully loaded"
          },
          "data": "aGVsbG8gd29ybGQ="
        },
        "headers": {
          "Content-Type": "application/json"
        }
      }
    },
    {
      "request": {
        "method": "POST",
        "url": "/api/v1/file-storage-management/upload",
        "queryParameters": {
          "bucketName": {
            "equalTo": "bucket-example"
          }
        }
      },
      "response": {
        "status": 200,
        "jsonBody": {
          "result": {
            "httpStatusCode": "OK",
            "status": "SUCCESS",
            "description": "File passport.jpg successfully uploaded"
          }
        },
        "headers": {
          "Content-Type": "application/json"
        }
      }
    },
    {
      "request": {
        "method": "DELETE",
        "url": "/api/v1/file-storage-management/bucket/delete",
        "queryParameters": {
          "bucketName": {
            "equalTo": "bucket-example"
          }
        }
      },
      "response": {
        "status": 200,
        "jsonBody": {
          "result": {
            "httpStatusCode": "NO CONTENT",
            "status": "SUCCESS",
            "description": "Bucket example-bucket deleted"
          }
        },
        "headers": {
          "Content-Type": "application/json"
        }
      }
    }
  ]
}



